{
  "name": "2ndmynd-workspace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "seed:jobber": "tsx scripts/seed/jobber_csv_to_ml_examples.ts --quotes ./seed/jobber/Quotes.csv --invoices ./seed/jobber/Invoices.csv",
    "ml:export-drafts": "tsx scripts/ml/export_drafts_for_review.ts --limit 20 --out ./ml_review/drafts.json",
    "ml:import-gold": "tsx scripts/ml/import_reviewed_gold.ts --in ./ml_review/drafts.json",
    "ml:finalize-train": "tsx scripts/ml/finalize_gold_and_freeze_train.ts --in ./ml_review/drafts.json",
    "ml:autofill-drafts": "tsx scripts/ml/autofill_drafts.ts --in ./ml_review/drafts.json --out ./ml_review/drafts.json",
    "ml:freeze-train": "tsx scripts/ml/freeze_dataset.ts --name train_v1 --purpose train --schema_version snapshot_v1 --quality approved",
    "ml:freeze-eval": "tsx scripts/ml/freeze_dataset.ts --name eval_v1 --purpose eval --schema_version snapshot_v1 --quality approved",
    "ml:export-jsonl": "tsx scripts/ml/export_dataset_to_jsonl.ts --name train_v1 --out ./ml_artifacts/train_v1.jsonl",
    "ml:finetune": "tsx scripts/ml/openai_finetune.ts --file ./ml_artifacts/train_v1.jsonl",
    "ml:finetune:4o-mini": "tsx scripts/ml/openai_finetune.ts --base_model gpt-4o-mini-2024-07-18 --suffix 2ndmynd-train-v1",
    "ml:smoke-test": "tsx scripts/ml/smoke_test_ft_model.ts --dataset train_v1 --n 20 --repeats 3 --strict",
    "ml:smoke-report": "tsx scripts/ml/smoke_report.ts",
    "ml:smoke-report:example": "tsx scripts/ml/smoke_report.ts --latest --by-example",
    "ml:infer": "tsx scripts/ml/infer_decision.ts",
    "ml:infer:v2": "tsx scripts/ml/infer_decision_v2.ts",
    "ml:diag-datasets": "tsx scripts/ml/diag_list_datasets.ts",
    "ml:extract-offenders": "tsx scripts/ml/extract_offenders.ts --smoke_cycle_id e0c9d1cc-b831-45ab-80e8-989ec4c0b882 --out ./ml_artifacts/offenders_latest.json",
    "ml:set-active-model": "tsx scripts/ml/set_active_model.ts --name decision_model --status active",
    "ml:list-offenders": "tsx scripts/ml/list_forbidden_offenders.ts --run_id 9b1493cc-6444-44f4-87ac-560cd41b4652 --out ./ml_artifacts/forbidden_offenders_latest.json",
    "ml:diag-examples": "tsx scripts/ml/diag_examples_summary.ts",
    "ml:rebuild-train": "tsx scripts/ml/rebuild_train_v1_from_seed.ts",
    "ml:scan-jsonl": "tsx scripts/ml/scan_and_clean_jsonl.ts --in ./ml_artifacts/train_v1.jsonl --out ./ml_artifacts/train_v1_clean.jsonl --report ./ml_artifacts/train_v1_clean_report.md",
    "ml:scan-jsonl:fail": "tsx scripts/ml/scan_and_clean_jsonl.ts --in ./ml_artifacts/train_v1.jsonl --report ./ml_artifacts/train_v1_clean_report.md --fail-on-find",
    "ml:gen-negatives": "tsx scripts/ml/generate_negative_examples.ts --seed_jsonl ./ml_artifacts/train_v1_clean.jsonl --offenders ./ml_artifacts/offenders_latest.json --out ./ml_artifacts/train_v2.jsonl --n 30",
    "ml:gen-negatives:v3": "tsx scripts/ml/generate_negative_examples.ts --seed_jsonl ./ml_artifacts/train_v1_clean.jsonl --offenders ./ml_artifacts/offenders_latest.json --out ./ml_artifacts/train_v3_with_negatives.jsonl --n 40",
    "ml:gen-decision:v2": "tsx scripts/ml/generate_decision_v2_dataset.ts",
    "ml:finetune:v3-neg": "tsx scripts/ml/openai_finetune.ts --file ./ml_artifacts/train_v3_with_negatives.jsonl --base_model gpt-4o-mini-2024-07-18 --suffix 2ndmynd-train-v3-neg --dataset train_v1",
    "ml:finetune:v2": "tsx scripts/ml/openai_finetune.ts --file ./ml_artifacts/train_v2.jsonl --suffix 2ndmynd-train-v2 --dataset train_v2",
    "ml:finetune:v3": "tsx scripts/ml/openai_finetune.ts --file ./ml_artifacts/train_v1.jsonl --base_model gpt-4o-mini-2024-07-18 --suffix 2ndmynd-train-v3 --dataset train_v1",
    "ml:kaggle:transform": "tsx scripts/ml/kaggle/transform_all.ts",
    "ml:kaggle:download": "tsx scripts/ml/kaggle/download_kaggle_dataset.ts",
    "ml:kaggle:ingest": "tsx scripts/ml/kaggle/run_kaggle_ingestion.ts",
    "ml:scenarios:generate": "tsx scripts/ml/generate_scenario_packs.ts --out_dir ./ml_scenarios --n_per_pattern 40 --include_negatives --hard_mode true --enable_fallback true --fallback_threshold 0.35",
    "ml:scenarios:import": "tsx scripts/ml/import_scenario_packs.ts --in ./ml_scenarios/packs.json",
    "ml:scenarios:coverage": "tsx scripts/ml/scenarios/coverage_report.ts --in ./ml_scenarios/packs.json",
    "ml:eval": "tsx scripts/ml/eval_runner.ts --dataset scenario_eval_v1",
    "ml:eval:baseline": "tsx scripts/ml/eval_runner.ts --dataset scenario_eval_v1 --mode full",
    "ml:eval:scenario": "tsx scripts/ml/eval_runner.ts --dataset scenario_eval_v1",
    "ml:eval:pattern": "tsx scripts/ml/eval_runner.ts --dataset scenario_eval_v1 --mode pattern_only",
    "ml:eval:compare": "tsx scripts/ml/eval_compare_models.ts --dataset scenario_eval_v1 --mode full",
    "ml:eval:decision:v2": "tsx scripts/ml/eval_decision_v2.ts",
    "ml:backfill:meta": "tsx scripts/ml/backfill_examples_meta.ts",
    "ml:diag:eval-db": "tsx scripts/ml/diag_eval_db.ts",
    "ml:scenarios:rebuild-eval": "tsx scripts/ml/rebuild_scenario_eval_v1.ts --packs_dir ./ml_scenarios",
    "ml:scenarios:verify-eval": "tsx scripts/ml/verify_scenario_eval_meta.ts",
    "ml:finetune:scenario": "tsx scripts/ml/openai_finetune_scenario.ts --train_dataset scenario_train_v1 --suffix 2ndmynd-scenario-v1",
    "ml:finetune:status": "tsx scripts/ml/openai_finetune_status.ts",
    "ml:finetune:events": "tsx scripts/ml/openai_finetune_events.ts",
    "ml:finetune:canary": "tsx scripts/ml/openai_finetune_scenario.ts --train_dataset scenario_train_v1 --suffix 2ndmynd-scenario-canary-v2 --epochs 2 --lr_multiplier 1.0 --max_wait_minutes 240",
    "ml:finetune:resume": "tsx scripts/ml/openai_finetune_scenario.ts --job_id",
    "ml:finetune:retry": "tsx scripts/ml/retry_scenario_finetune.ts",
    "ml:release-gate": "tsx scripts/ml/release_gate.ts --eval_dataset scenario_eval_v1 --activate true",
    "ml:scenarios:generate:canary": "tsx scripts/ml/generate_scenario_packs.ts --out_dir ./ml_scenarios --n_per_pattern 10 --include_negatives --hard_mode true --enable_fallback true --fallback_threshold 0.35 --only_patterns scheduling_window_pressure,quote_followup_drag,cash_timing_stretch,scope_instability_midjob --max_total_packs 80",
    "ml:scenario:canary-loop": "tsx scripts/ml/scenario_canary_loop.ts --n_per_pattern 25 --max_total_packs 220 --suffix 2ndmynd-scenario-canary-v2",
    "ml:ft:create": "tsx scripts/ml/create_job_v2.ts",
    "ml:ft:start": "tsx scripts/ml/start_ft_v2.ts",
    "ml:ft:watchdog": "tsx scripts/ml/create_or_watchdog.ts",
    "ml:ft:watchdog:mini": "cross-env FT_BASE_MODEL=gpt-4.1-mini tsx scripts/ml/create_or_watchdog.ts",
    "ml:ft:watchdog:4o-mini": "cross-env FT_BASE_MODEL=gpt-4o-mini-2024-07-18 tsx scripts/ml/create_or_watchdog.ts"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.11",
