{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ModelEntry",
  "type": "object",
  "additionalProperties": false,
  "required": ["model_id", "created_at", "dataset_hash", "status"],
  "properties": {
    "model_id": { "type": "string" },
    "job_id": { "type": "string" },
    "created_at": { "type": "string", "format": "date-time" },
    "dataset_hash": { "type": "string" },
    "eval_report_path": { "type": "string" },
    "status": { "enum": ["candidate", "champion", "rejected", "training"] }
  }
}
